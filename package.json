{
  "name": "elm-package-prefixer",
  "version": "1.0.0",
  "description": "Prefix existing elm packages to avoid namespace collisions.",
  "main": "index.js",
  "bin": {
    "elm-package-prefixer": "./index.js"
  },
  "scripts": {
    "build": "NODE_ENV=production node build.js",
    "build:dev": "NODE_ENV=development node build.js",
    "postinstall": "patch-package"
  },
  "keywords": [
    "elm",
    "prefix",
    "prefixer",
    "cli",
    "tool"
  ],
  "author": "Andy Thompson",
  "license": "MIT",
  "devDependencies": {
    "node-elm-compiler": "^5.0.4",
    "parcel-bundler": "^1.12.4",
    "patch-package": "^6.2.1",
    "@babel/compat-data": "^7.9.0"
  },
  "dependencies": {
    "node-fetch": "^3.1.1",
    "xhr2-unsafe": "^0.1.5"
  }
}
